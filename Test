// ---------------- PARAMETERIZED TEST ----------------
@ParameterizedTest
@ValueSource(booleans = {true, false})
void testParameterizedAttendancePresentFlag(Boolean presentFlag) {
    Attendance a = new Attendance();
    a.setPresent(presentFlag);

    assertNotNull(a.getPresent());
    assertEquals(presentFlag, a.getPresent());
}

// ---------------- DISABLED TEST ----------------
@Disabled("Example disabled test for Attendance DAO")
@Test
void testDisabledAttendanceExample() {
    fail("This test is intentionally disabled.");
}


// ---------------- PARAMETERIZED TEST ----------------
@ParameterizedTest
@ValueSource(ints = {1, 3, 5})
void testParameterizedFeedbackRatings(int rating) {
    Feedback f = new Feedback();
    f.setRating(rating);

    assertNotNull(f.getRating());
    assertTrue(f.getRating() >= 1 && f.getRating() <= 5);
}

// ---------------- DISABLED TEST ----------------
@Disabled("Example disabled test for Feedback DAO")
@Test
void testDisabledFeedbackExample() {
    fail("This test is intentionally disabled.");
}


// ---------------- PARAMETERIZED TEST ----------------
@ParameterizedTest
@ValueSource(strings = {"Java Batch", "AI Batch", "ML Batch"})
void testParameterizedBatchNames(String batchName) {
    InternshipBatch batch = new InternshipBatch();
    batch.setBatchName(batchName);

    assertNotNull(batch.getBatchName());
    assertTrue(batch.getBatchName().contains("Batch"));
}


const handleDelete = async (batchName) => {
  try {
    await deleteBatchByName(batchName);
    setMsg("Deleted " + batchName);
    fetch();
  } catch (e) {
    const errorMsg =
      e?.response?.data?.error ||   // backend returns {error: "..."}
      e?.response?.data?.message || // backend returns {message: "..."}
      e?.response?.data || 
      e.message;

    setMsg(String(errorMsg)); // convert to plain string
  }
};ALTER TABLE registrations
DROP FOREIGN KEY fk_batchcode;ALTER TABLE registrations
ADD CONSTRAINT fk_batchcode
FOREIGN KEY (batch_code)
REFERENCES internship_batches(batch_code)
ON DELETE CASCADE;
